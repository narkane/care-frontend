{
  "name": "oec-frontend",
  "version": "0.0.1",
  "description": "W4A.care - EmojiMap",
  "repository": {
    "type": "git",
    "url": "https://github.com/narkane/oec-frontend.git"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve --env VUE_APP_API_URL=https://localhost:3000",
    "build": "npm run generate-build-version && vue-cli-service build",
    "format": "prettier --write --config .prettierrc \"**/*.js\"",
    "lint": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:unit": "vue-cli-service test:unit",
    "remark": "remark . -o",
    "ci": "vue-cli-service test:e2e --env API_URL=https://api-demo.daniel-avellaneda.com,ENV=ci --headless --config video=false",
    "coverage": "nyc report --reporter=text-summary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run remark"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@mdi/font": "^5.9.55",
    "@popperjs/core": "^2.11.6",
    "@vueuse/components": "^9.5.0",
    "@vueuse/core": "^9.5.0",
    "axios": "^0.21.4",
    "babel": "^6.23.0",
    "babel-loader": "8.0.2",
    "bootstrap": "^5.2.2",
    "core-js": "^3.26.1",
    "cypress": "^11.1.0",
    "date-fns": "^2.29.3",
    "ejs": "^3.1.8",
    "l": "^0.6.0",
    "prettier": "^2.7.1",
    "projections": "^1.0.0",
    "remark-cli": "^8.0.1",
    "vee-validate": "^3.4.14",
    "vue": "2.7.14",
    "vue-analytics": "^5.22.1",
    "vue-country-flag": "^2.3.2",
    "vue-i18n": "^8.28.2",
    "vue-material": "^1.0.0-beta-15",
    "vue-meta": "^2.4.0",
    "vue-resize-text": "^0.1.1",
    "vue-router": "^3.6.5",
    "vue-toastify": "^1.8.1",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.6.12",
    "vuex": "^3.6.2",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/babel-preset-app": "^4.5.19",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-e2e-cypress": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-plugin-unit-mocha": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.3.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "faker": "^5.5.3",
    "fibers": "^5.0.3",
    "husky": "^4.3.8",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": "^10.5.4",
    "nyc": "^15.1.0",
    "sass": "~1.32.13",
    "sass-loader": "^10.4.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.3",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.7.14",
    "vuetify-loader": "^1.9.2"
  },
  "keywords": [
    "vuejs",
    "vue-router",
    "vuex",
    "vuetify",
    "javascript",
    "i18n",
    "eslint",
    "skeleton",
    "async",
    "starter",
    "webapp",
    "web",
    "app",
    "await",
    "mvp",
    "frontend",
    "testing",
    "prettier",
    "cypress",
    "mocha",
    "chai"
  ]
}
